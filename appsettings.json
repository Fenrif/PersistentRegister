{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost; Database=RegistrationDB; Integrated Security=false; User Id=sa; Password=password1!; TrustServerCertificate=true;"
  },
  "AppSettings": {
    "FileStorageFile": "fileStorage/fileDB.json"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/myLogs-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}